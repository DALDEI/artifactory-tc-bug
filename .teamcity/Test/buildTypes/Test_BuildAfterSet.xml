<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="23ced35e-0766-4eb9-b633-c5f5b445ea80" paused="true" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>build-after-set</name>
  <description>Build using dependant build</description>
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.Test_TestBuildnumberSet.build.number%" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters>
      <param name="repo-resolve-dev" value="bogus" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_18" name="printversion" type="simpleRunner">
        <parameters>
          <param name="org.jfrog.artifactory.selectedDeployableServer.buildDependencies" value="Requires Artifactory Pro." />
          <param name="org.jfrog.artifactory.selectedDeployableServer.deployReleaseFlag" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.envVarsExcludePatterns" value="*password*,*secret*" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.publishBuildInfo" value="true" />
          <param name="script.content"><![CDATA[echo build-number is %build.number%
echo dependant version is %dep.Test_TestBuildnumberSet.currentVersion%%]]></param>
          <param name="teamcity.build.workingDir" value="gradle-example-ci-server2" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_21" name="buildAll" type="gradle-runner">
        <parameters>
          <param name="org.jfrog.artifactory.selectedDeployableServer.activateGradleIntegration" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.alternativeGradleTasks" value="build" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.defaultModuleVersionConfiguration" value="GLOBAL" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.deployArtifacts" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.enableReleaseManagement" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.envVarsExcludePatterns" value="*password*,*secret*" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.gitReleaseBranchNamePrefix" value="releasex-" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.nextIntegrationProperties" value="integrationVersion" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.publishBuildInfo" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.publishIvyDescriptors" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.publishMavenDescriptors" value="true" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.releaseProperties" value="currentVersion" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.resolvingRepo" value="libs-releases" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.targetRepo" value="artifactory-bug-release" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.urlId" value="0" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.useM2CompatiblePatterns" value="true" />
          <param name="teamcity.build.workingDir" value="gradle-example-ci-server2" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRUnner.gradle.build.file" value="build.gradle" />
          <param name="ui.gradleRunner.gradle.stacktrace.enabled" value="true" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="build" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="NexstraGithub" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="Test_TestBuildnumberSet">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

