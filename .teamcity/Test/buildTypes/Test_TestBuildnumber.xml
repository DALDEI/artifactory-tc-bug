<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="923b8d4b-9633-4c12-8350-fba4fc7a1d3e" paused="true" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>test-buildnumber</name>
  <description />
  <settings>
    <options>
      <option name="buildNumberPattern" value="%sharedBuildNumber.id1%" />
      <option name="checkoutMode" value="ON_SERVER" />
    </options>
    <disabled-settings>
      <setting-ref ref="RUNNER_7" />
    </disabled-settings>
    <parameters>
      <param name="repo-resolve-dev" value="libs-release" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="echo build" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[echo id1 %sharedBuildNumber.id1%
echo id1.next %sharedBuildNumber.id1.next%]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_7" name="xmlsh" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[#!/bin/env /usr/local/bin/xmlsh
xversion
set]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="NexstraGithub" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

